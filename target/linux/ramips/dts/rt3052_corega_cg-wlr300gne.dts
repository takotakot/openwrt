/dts-v1/;

#include "rt3050.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "corega,cg-wlr300gne", "ralink,rt3052-soc";
	model = "Corega CG-WLR300GNE";

	aliases {
		led-boot = &led_power_green;
		led-failsafe = &led_power_amber;
		led-running = &led_power_green;
		led-upgrade = &led_power_green;
	};

	flash@1f000000 {
		compatible = "cfi-flash";
		reg = <0x1f000000 0x800000>;
		bank-width = <2>;
		device-width = <2>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x40000>;
				read-only;
			};

			partition@40000 {
				label = "u-boot-env";
				reg = <0x40000 0x10000>;
				read-only;
			};

			factory: partition@50000 {
				label = "factory";
				reg = <0x50000 0x10000>;
				read-only;
			};

			partition@60000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x60000 0x780000>;
			};

			partition@7e0000 {
				label = "NVRAM";
				reg = <0x7e0000 0x10000>;
			};

			partition@7f0000 {
				label = "NVRAM_FACTORY";
				reg = <0x7f0000 0x10000>;
			};
		};
	};

	leds {
		compatible = "gpio-leds";

		wired {
			label = "cg-wlr300gne:green:wired";
			gpios = <&gpio0 4 GPIO_ACTIVE_HIGH>;
		};

		led_power_green: power_green {
			label = "cg-wlr300gne:green:power";
			gpios = <&gpio0 5 GPIO_ACTIVE_LOW>;
		};

		led_power_amber: power_amber {
			label = "cg-wlr300gne:amber:power";
			gpios = <&gpio0 6 GPIO_ACTIVE_LOW>;
		};

		internet {
			label = "cg-wlr300gne:green:internet";
			gpios = <&gpio0 8 GPIO_ACTIVE_LOW>;
		};

		status {
			label = "cg-wlr300gne:red:status";
			gpios = <&gpio0 12 GPIO_ACTIVE_LOW>;
		};

		wps {
			label = "cg-wlr300gne:green:wps";
			gpios = <&gpio0 13 GPIO_ACTIVE_LOW>;
		};

		usb {
			label = "cg-wlr300gne:green:usb";
			gpios = <&gpio0 17 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "usbport";
			trigger-sources = <&otg_port1>;
		};
	};

	keys {
		compatible = "gpio-keys";

		wps {
			label = "wps";
			gpios = <&gpio0 0 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};

		router_on {
			label = "router_on";
			gpios = <&gpio0 7 GPIO_ACTIVE_LOW>;
			linux,input-type = <EV_SW>;
			linux,code = <BTN_0>;
		};

		reset {
			label = "reset";
			gpios = <&gpio0 9 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		multiap_on {
			label = "multiap_on";
			gpios = <&gpio0 19 GPIO_ACTIVE_LOW>;
			linux,input-type = <EV_SW>;
			linux,code = <BTN_1>;
		};

		usb {
			label = "usb";
			gpios = <&gpio0 20 GPIO_ACTIVE_LOW>;
			linux,code = <BTN_2>;
		};
	};

	rtl8366rb {
		compatible = "realtek,rtl8366rb";
		gpio-sda = <&gpio0 1 GPIO_ACTIVE_HIGH>;
		gpio-sck = <&gpio0 2 GPIO_ACTIVE_HIGH>;
	};
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			ralink,group = "spi", "i2c", "jtag", "mdio", "uartf";
			ralink,function = "gpio";
		};
	};
};

&ethernet {
	mtd-mac-address = <&factory 0x4>;
	pinctrl-names = "default";
	pinctrl-0 = <&rgmii_pins>;
};

&esw {
//	ralink,rgmii = <1>;
	/*
	 * ext phy base addr 31, rx/tx clock skew 0,
	 * turbo mii off, rgmi 3.3v off, port 5 polling off
	 * port5: enabled, gige, full-duplex, rx/tx-flow-control
	 * port6: enabled, gige, full-duplex, rx/tx-flow-control
	*/
	ralink,fpa2 = <0x1f503fff>;
};

&wmac {
	ralink,mtd-eeprom = <&factory 0x0>;
};

&otg {
	status = "okay";
};
