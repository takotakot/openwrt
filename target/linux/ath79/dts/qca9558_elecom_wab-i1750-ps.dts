/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "qca955x.dtsi"

/ {
	compatible = "elecom,wab-i1750-ps", "qca,qca9557";
	model = "ELECOM WAB-I1750-PS";

	aliases {
		serial1 = &uart1;
		led-boot = &led_status;
		led-failsafe = &led_status;
		led-upgrade = &led_status;
	};

	chosen {
		bootargs = "console=ttyS0,115200n8 console=ttyATH1,115200n8";
	};

	leds {
		compatible = "gpio-leds";

		wlan2g {
			label = "wab-i1750-ps:green:wlan2g";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy1tpt";
		};

		led_status: status {
			label = "wab-i1750-ps:red:status";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		};

		usb {
			label = "wab-i1750-ps:green:usb";
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
			trigger-sources = <&hub_port>;
			linux,default-trigger = "usbport";
		};
	};

	keys {
		compatible = "gpio-keys";

		wps {
			label = "wps";
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
			debounce-interval = <60>;
		};

		reset {
			label = "reset";
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
			debounce-interval = <60>;
		};

		eject {
			label = "eject";
			gpios = <&gpio 19 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_EJECTCD>;
			debounce-interval = <60>;
		};
	};

	beeper {
		compatible = "gpio-beeper";
		gpios = <&gpio 4 GPIO_ACTIVE_HIGH>;
	};

	reg_usb_vbus: regulator {
		compatible = "regulator-fixed";
		regulator-name = "usb_vbus";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		gpio = <&gpio 11 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		regulator-always-on;
	};

	watchdog {
		compatible = "linux,wdt-gpio";
		gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
		hw_algo = "toggle";
		hw_margin_ms = <300>;
		always-running;
	};

	ahb {
		apb {
			uart1: uart@18500000 {
				compatible = "qca,ar9330-uart";
				reg = <0x18500000 0x14>;

				pinctrl-names = "default";
				pinctrl-0 = <&pinmux_uart1 &jtag_disable_pins>;

				interrupts = <6>;
				interrupt-parent = <&miscintc>;

				clocks = <&pll ATH79_CLK_UART1>;
				clock-names = "uart";
			};
		};
	};
};

&spi {
	status = "okay";

	num-cs = <1>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x000000 0x040000>;
				read-only;
			};

			partition@40000 {
				label = "u-boot-env";
				reg = <0x040000 0x010000>;
				read-only;
			};

			partition@50000 {
				label = "art";
				reg = <0x050000 0x010000>;
				read-only;
			};

			partition@60000 {
				label = "art2";
				reg = <0x060000 0x010000>;
				read-only;
			};

			partition@70000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x070000 0xe00000>;
			};

			partition@e70000 {
				label = "manufacture";
				reg = <0xe70000 0x100000>;
				read-only;
			};

			partition@f70000 {
				label = "backup";
				reg = <0xf70000 0x010000>;
				read-only;
			};

			partition@f80000 {
				label = "storage";
				reg = <0xf80000 0x080000>;
				read-only;
			};
		};
	};
};

&mdio0 {
	status = "okay";

	phy4: ethernet-phy@4 {
		reg = <4>;
		eee-broken-100tx;
		eee-broken-1000t;
	};
};

&eth0 {
	status = "okay";

	phy-handle = <&phy4>;
	pll-data = <0xa6000000 0x80000101 0x80001313>;

	gmac_config: gmac-config {
		device = <&gmac>;

		rxdv-delay = <0>;
		rxd-delay = <0>;
		txen-delay = <0>;
		txd-delay = <0>;
		rgmii-enabled = <1>;
	};
};

&mdio1 {
	status = "okay";

	phy0: ethernet-phy@1 {
		reg = <1>;
		at803x-override-sgmii-link-check;
	};
};

&eth1 {
	status = "okay";

	phy-handle = <&phy0>;
	pll-data = <0x03000000 0x00000101 0x00001313>;

	qca955x-sgmii-fixup;
};

&pcie0 {
	status = "okay";

	wifi@0,0 {
		compatible = "qcom,ath10k";
		reg = <0x0000 0 0 0 0>;
	};
};

&gpio {
	/*
	 * U-Boot doesn't configure uart1, configuring GPIO_OE is needed for
	 * four pins of uart1 (0-TD/2-RTS: OUT, 1-RD/3-CTS: IN)
	 */
	uart1_out {
		gpio-hog;
		gpios = <0 GPIO_ACTIVE_HIGH>,
			<2 GPIO_ACTIVE_HIGH>;
		output-low;
		line-name = "uart1-td-rts";
	};

	uart1_in {
		gpio-hog;
		gpios = <1 GPIO_ACTIVE_HIGH>,
			<3 GPIO_ACTIVE_HIGH>;
		input;
		line-name = "uart1-rd-cts";
	};
};

&pinmux {
	pinmux_uart1: uart1 {
		// UART1_TD on GPIO 0, UART1_RD on GPIO 1,
		// UART1_RTS on GPIO 2, UART1_CTS on GPIO 3
		pinctrl-single,bits = <0x0 0x130012 0xff00ff>,
				<0x3c 0x3010000 0xffff0000>;
	};
};

&uart {
	status = "okay";
};

&usb_phy0 {
	status = "okay";
};

&usb0 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	dr_mode = "host";
	vbus-supply = <&reg_usb_vbus>;

	hub_port: port@1 {
		reg = <1>;
		#trigger-source-cells = <0>;
	};
};

&wdt {
	status = "disabled";
};

&wmac {
	status = "okay";

	qca,no-eeprom;
};
