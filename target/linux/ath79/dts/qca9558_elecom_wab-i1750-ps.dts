	uart@18500000 {
		compatible = "qca,ar9330-uart";
		reg = <0x18500000 0x14>;

		interrupts = <3>;

		clocks = <&pll ATH79_CLK_REF>;
		clock-names = "uart";
	};
};

&spi {
	status = "okay";

	num-cs = <1>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x000000 0x040000>;
				read-only;
			};

			partition@40000 {
				label = "u-boot-env";
				reg = <0x040000 0x010000>;
				read-only;
			};

			partition@50000 {
				label = "art";
				reg = <0x050000 0x010000>;
				read-only;
			};

			partition@60000 {
				label = "art2";
				reg = <0x060000 0x010000>;
				read-only;
			};

			partition@70000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x070000 0xe00000>;
			};

			partition@e70000 {
				label = "manufacture";
				reg = <0xe70000 0x100000>;
				read-only;
			};

			partition@f70000 {
				label = "backup";
				reg = <0xf70000 0x010000>;
				read-only;
			};

			partition@f80000 {
				label = "manufacture";
				reg = <0xf80000 0x080000>;
				read-only;
			};
		};
	};
};

&mdio0 {
	status = "okay";

	phy4: ethernet-phy@4 {
		reg = <4>;
//		eee-broken-100tx;
//		eee-broken-1000t;
	};
};

&eth0 {
	status = "okay";

//	mtd-mac-address = <&art 0x0>;
	phy-handle = <&phy4>;
	pll-data = <0xa6000000 0x00000101 0x00001616>;

	gmac_config: gmac-config {
		device = <&gmac>;

		rxdv-delay = <3>;
		rxd-delay = <3>;
		txen-delay = <0>;
		txd-delay = <0>;
		rgmii-enabled = <1>;
	};
};

&mdio1 {
	status = "okay";

	phy0: ethernet-phy@0 {
		reg = <0>;
//		at803x-override-sgmii-link-check;
	};
};

&eth1 {
	status = "okay";

//	mtd-mac-address = <&art 0x6>;
	phy-handle = <&phy0>;
	pll-data = <0x00000101 0x00000101 0x00001616>;
};

