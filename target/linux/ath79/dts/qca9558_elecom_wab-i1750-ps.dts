/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "qca955x.dtsi"

/ {
	compatible = "elecom,wab-i1750-ps", "qca,qca9557";
	model = "ELECOM WAB-I1750-PS";

	aliases {
		led-boot = &led_status;
		led-failsafe = &led_status;
		led-upgrade = &led_status;
	};

	leds {
		compatible = "gpio-leds";

		wlan2g {
			label = "wab-i1750-ps:green:wlan2g";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy1tpt";
		};

		led_status: status {
			label = "wab-i1750-ps:red:status";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		};

		usb {
			label = "wab-i1750-ps:green:usb";
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
			trigger-sources = <&hub_port>;
			linux,default-trigger = "usbport";
		};
	};

	keys {
		compatible = "gpio-keys";

		wps {
			label = "wps";
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
			debounce-interval = <60>;
		};

		reset {
			label = "reset";
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
			debounce-interval = <60>;
		};

		eject {
			label = "eject";
			gpios = <&gpio 19 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_EJECTCD>;
			debounce-interval = <60>;
		};
	};

	watchdog {
		compatible = "linux,wdt-gpio";
		gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
		hw_algo = "toggle";
		hw_margin_ms = <300>;
		always-running;
	};

	uart@18500000 {
		compatible = "qca,ar9330-uart";
		reg = <0x18500000 0x14>;

		interrupts = <3>;

		clocks = <&pll ATH79_CLK_REF>;
		clock-names = "uart";
	};
};

&spi {
	status = "okay";

	num-cs = <1>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x000000 0x040000>;
				read-only;
			};

			partition@40000 {
				label = "u-boot-env";
				reg = <0x040000 0x010000>;
				read-only;
			};

			partition@50000 {
				label = "art";
				reg = <0x050000 0x010000>;
				read-only;
			};

			partition@60000 {
				label = "art2";
				reg = <0x060000 0x010000>;
				read-only;
			};

			partition@70000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x070000 0xe00000>;
			};

			partition@e70000 {
				label = "manufacture";
				reg = <0xe70000 0x100000>;
				read-only;
			};

			partition@f70000 {
				label = "backup";
				reg = <0xf70000 0x010000>;
				read-only;
			};

			partition@f80000 {
				label = "manufacture";
				reg = <0xf80000 0x080000>;
				read-only;
			};
		};
	};
};

&mdio0 {
	status = "okay";

	phy4: ethernet-phy@4 {
		reg = <4>;
//		eee-broken-100tx;
//		eee-broken-1000t;
	};
};

&eth0 {
	status = "okay";

//	mtd-mac-address = <&art 0x0>;
	phy-handle = <&phy4>;
	pll-data = <0xa6000000 0x00000101 0x00001616>;

	gmac_config: gmac-config {
		device = <&gmac>;

		rxdv-delay = <3>;
		rxd-delay = <3>;
		txen-delay = <0>;
		txd-delay = <0>;
		rgmii-enabled = <1>;
	};
};

&mdio1 {
	status = "okay";

	phy0: ethernet-phy@1 {
		reg = <1>;
//		at803x-override-sgmii-link-check;
	};
};

&eth1 {
	status = "okay";

//	mtd-mac-address = <&art 0x6>;
	phy-handle = <&phy0>;
	pll-data = <0x00000101 0x00000101 0x00001616>;
};

&pcie0 {
	status = "okay";

	wifi@0,0 {
		compatible = "qcom,ath10k";
		reg = <0x0000 0 0 0 0>;
	};
};

&uart {
	status = "okay";
};

&usb_phy0 {
	status = "okay";
};

&usb0 {
	status = "okay";

	hub_port: port@1 {
		reg = <1>;
		#trigger-source-cells = <0>;
	};
};

&wdt {
	status = "disabled";
};

&wmac {
	status = "okay";

	qca,no-eeprom;
};
